	"sourceRoot": "libs/student-submissions/frontend/common-models/src",
	"prefix": "rspd",
	"projectType": "library",
	"targets": {
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
